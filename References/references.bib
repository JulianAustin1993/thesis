	author = {Muro, Javier and Canty, Morton and Conradsen, Knut and Hüttich, Christian and Nielsen, Allan Aasbjerg and Skriver, Henning and Remy, Florian and Strauch, Adrian and Thonfeld, Frank and Menz, Gunter},
	month = oct,
