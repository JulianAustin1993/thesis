@inproceedings{hooker_maximal_2015,
	title = {Maximal autocorrelation factors for function-valued spatial/temporal data},
	isbn = {978-0-9872143-5-5},
	url = {http://www.mssanz.org.au/modsim2015/A3/hooker.pdf},
